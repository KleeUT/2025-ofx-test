{
  "name": "be-test",
  "version": "0.1.0",
  "bin": {
    "be-test": "dist/bin/be-test.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy --require-approval=never",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint src/**/* test/**/*",
    "lint:fix": "eslint src/**/* test/**/* --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/jest": "^29.5.14",
    "@types/node": "20.14.10",
    "@types/prettier": "2.7.3",
    "aws-cdk": "^2.1026.0",
    "esbuild": "0.23.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "29.7.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.40.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.613.0",
    "@aws-sdk/lib-dynamodb": "3.613.0",
    "@types/aws-lambda": "8.10.141",
    "aws-cdk-lib": "^2.212.0",
    "constructs": "10.3.0",
    "husky": "^9.1.7",
    "source-map-support": "0.5.21"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
